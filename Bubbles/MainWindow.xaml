<Window x:Class="Bubbles.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        ShowInTaskbar="False" ResizeMode="NoResize" WindowStyle="None"
        Loaded="Window_Loaded" MouseDown="Window_MouseDown" KeyDown="Window_KeyDown">
    <Grid x:Name="MainGrid">
        <Canvas x:Name="MainCanvas" Margin="0" SizeChanged="MainCanvas_SizeChanged"/>
    </Grid>
</Window>
